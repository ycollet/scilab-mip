  <refnamediv>
    <refname>ff_exec</refname>
    <refpurpose>execute a FreeFem statement</refpurpose>
  </refnamediv>
  
  <refsynopsisdiv>
    <title>Calling Sequence</title>
    <synopsis>ff_exec(string)</synopsis>
  </refsynopsisdiv>
  
  <refsection>
    <title>Parameters</title>
    <variablelist>
      <varlistentry>
	<term>string</term>
	<listitem>
	  <para>a string containing the statement to be executed in FreeFem</para>
	</listitem>
      </varlistentry>
    </variablelist>
  </refsection>
  
   <refsection>
     <title>Description</title>

     <para>execute a FreeFem statement</para>
   </refsection>
   
   <refsection>
     <title>Examples</title>
     <programlisting role="example"><![CDATA[ 
ff_exec('mesh th1 = movemesh(th, x-u1, y-u2);');
 ]]></programlisting>
   </refsection>

