	  version="5.0-subset Scilab" 
	  xml:lang="en" 
	  xml:id="lhsamp">
  <info>
    <pubdate>5-Dec-2007</pubdate>
  </info>
  <refnamediv>
    <refname>lhsamp</refname>
    <refpurpose> Latin hypercube distributed random numbers </refpurpose>
  </refnamediv>
  
  <refsynopsisdiv>
    <title>Calling Sequence</title>
    <synopsis>S = lhsamp(m,n)</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Parameters</title>
    <variablelist>
      <varlistentry>
        <term>m</term>
        <listitem>
          <para>number of sample points to generate, if unspecified m = 1</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>n</term>
        <listitem>
          <para>number of dimensions, if unspecified n = m</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>S</term>
        <listitem>
          <para>the generated n dimensional m sample points chosen from uniform distributions on m subdivions of the interval (0.0, 1.0)</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>
  
  <refsection>
    <title>Description</title>
    <para>Latin hypercube distributed random numbers</para>
  </refsection>
  
  <refsection>
    <title>See Also</title>
