  <refnamediv>
    <refname>doe_prime</refname>

    <refpurpose> Returns any of the first PRIME_MAX (1600) prime
    numbers.</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Calling Sequence</title>

    <synopsis>p = doe_prime(n)</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Parameters</title>

    <variablelist>
      <varlistentry>
        <term>n</term>

        <listitem>
          <para>the rank of the prime number to be returned</para>
        </listitem>
      </varlistentry>

      <varlistentry>
        <term>p</term>

        <listitem>
          <para>the nth stored prime number</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>

  <refsection>
    <title>Description</title>

    <para>Returns any of the first PRIME_MAX (1600) prime numbers.</para>
  </refsection>

  <refsection>
    <title>Examples</title>

    <programlisting role="example"> 
disp(doe_prime(10))
disp(doe_prime(1234))
    </programlisting>
  </refsection>

